{
  "ipc_rest_adapters":
  [
    {
      "adapter_name": "nodes_status",
      "ipc_queue": "/nodes_status",
      "rest_route": "/nodes_status",
      "group_by_attrib": "ip_address"
    },
    {
      "adapter_name": "sensor_status",
      "ipc_queue": "/sensor_status",
      "rest_route": "/sensor_status"
    }
  ],
  "rest_actions":
  [
    {
      "action_name": "power_on",
      "rest_route": "/power_on",
      "script_path": "/nfs/scripts/automation/lisa_scripts/power_on.py",
      "argument_list": [{"name": "node_num", "type": "int"}]
    },
    {
      "action_name": "reset",
      "rest_route": "/reset",
      "script_path": "/nfs/scripts/automation/lisa_scripts/reset.py",
      "argument_list": [{"name": "node_num", "type": "int"}]
    },
    {
      "action_name": "new_user",
      "rest_route": "/new_user",
      "script_path": "/nfs/scripts/automation/lisa_scripts/new_user.py",
      "argument_list": [{"name": "username", "type": "string"}, {"name": "password", "type": "string"}]
    },
    {
      "action_name": "users",
      "rest_route": "/users",
      "script_path": "/nfs/scripts/automation/lisa_scripts/users.py",
      "argument_list": []
    }
  ],
  "log_level": "DEBUG"
}
